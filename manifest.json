{
    "author": "gellel",
	"description": "a small encyclopedia about emoji's!",
	"programs": {
		"files": {
            "description": "make, manage or remove file dependencies used by the emojipedia program.",
            "programs": {
                "categories": {
                    "description": "manage the files used by the emojipedia categories program."
                },
                "emojis": {
                    "description": "manage the files used by the emoji's program."
                },
                "keywords": {
                    "description": "manage the files used by the emoji's keywords program."
                },
                "subcategory": {
                    "description": "manages the files used by the emojipedia subcategories program."
                }
            }
        },
		"get": {
			"description": "fetch information from the emojipedia.",
            "programs": {
                "categories":{
                    "description": "get information by emoji category."
                },
                "emoji": {
                    "description": "get fragments of information about a specific emoji.",
                    "programs": {
                        "category": {
                            "description": "get the emoji's category."
                        },
                        "character": {
                            "description": "get the emoji's unicode character."
                        },
                        "codes": {
                            "description": "get the emoji's unicode sequence."
                        },
                        "description": {
                            "description": "get the emoji's emojipedia.org description."
                        },
                        "keywords": {
                            "description": "get the associated keywords for the emoji."
                        },
                        "number": {
                            "description": "get the emoji's numeric ID."
                        },
                        "subcategory": {
                            "description": "get the emoji's subcategory."
                        }
                    }
                },
                "keywords": {
                    "description": "get information by emoji keywords."
                },
                "subcategory": {
                    "description": "get information by emoji subcategory."
                }
            }
        }
	},
    "name": "emojipedia",
    "version": 1
}